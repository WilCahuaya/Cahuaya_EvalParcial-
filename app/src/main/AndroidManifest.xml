<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cahuaya_evalparcial">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cahuaya_EvalParcial"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".NavDraw_Curso_Cahuaya"
            android:label="@string/title_activity_nav_draw_curso_cahuaya"
            android:theme="@style/Theme.Cahuaya_EvalParcial.NoActionBar"
            android:exported="true">
        </activity>
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>